option domain-name-servers 20.30.40.50 ;
option domain-name "notascam.com" ;
option routers 192.168.0.1 ;

subnet 192.168.0.0 netmask 255.255.255.0 {
       range 192.168.0.50 192.168.0.100;
       }

host www { hardware ethernet 06:89:ee:98:59:a0; fixed-address 192.168.0.5 ;}
host dnsnot { hardware ethernet 46:7f:d3:f5:04:8a; fixed-address 192.168.0.4 ;}
host mailnot { hardware ethernet e2:6d:f5:69:bf:3c;  fixed-address 192.168.0.6 ;}